{
  "name": "root",
  "private": true,
  "workspaces": [
    "auto-nested-routes/*",
    "html-entry/*",
    "nested-routes/*",
    "parcel/*",
    "sandbox/*",
    "switcher/*"
  ],
  "devEngines": {
    "node": ">=12.22"
  },
  "scripts": {
    "run:parcel": "lerna run start --scope @case-parcel/**",
    "run:switcher": "lerna run start --scope @case-switcher/**",
    "run:sandbox": "lerna run start --scope @case-sandbox/**",
    "run:nested-routes": "lerna run start --scope @case-nested-routes/**",
    "run:auto-nested-routes": "lerna run start --scope @case-auto-nested-routes/**",
    "run:html-entry": "lerna run start --scope @case-html-entry/**",
    "lint": "eslint ."
  },
  "dependencies": {
    "@versea/core": "0.3.0",
    "@versea/plugin-auto-wait-container": "0.3.0",
    "@versea/plugin-html-entry": "0.3.0",
    "@versea/plugin-sandbox": "0.3.0",
    "@versea/plugin-source-entry": "0.3.0",
    "@versea/shared": "0.3.0",
    "@versea/tapable": "0.3.0",
    "@versea/versea": "0.3.0",
    "@vue/cli": "^5.0.4",
    "create-react-app": "^5.0.0",
    "eslint-config-react-app": "^7.0.1",
    "inversify": "^6.0.1",
    "lerna": "^4.0.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "@vue/eslint-config-standard": "^6.1.0",
    "@vue/eslint-config-typescript": "^9.1.0",
    "eslint": "^7.32.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-vue": "^8.0.3",
    "typescript": "~4.5.5",
    "vue-template-compiler": "^2.6.14"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/versea/versea-examples.git"
  }
}